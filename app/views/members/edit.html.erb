<% provide(:title, "Edit Member") %>
<div class="container-fluid" id="main">
<div class="row">

  <h1>Editing member No. <%= @member.id %></h1>

  <%= render 'form', member: @member %>

  <div>
    <%= link_to 'Show Member', @member, type: "button", 
      class: "btn btn-xs" %>
    <%= link_to 'Members List', members_path, type: "button",
      class: "btn btn-xs" %>
  </div>

</div>  <!-- row -->
</div> <!-- container-fluid -->
